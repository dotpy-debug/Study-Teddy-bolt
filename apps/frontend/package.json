{
  "name": "@studyteddy/frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@latest",
  "scripts": {
    "dev": "bunx next dev",
    "build": "bunx next build",
    "build:analyze": "ANALYZE=true bunx next build",
    "start": "bunx next start",
    "lint": "bunx eslint",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "generate": "bun .next-agent/scripts/generate.js",
    "audit": "bun .next-agent/scripts/audit.js",
    "agent:component": "bun .next-agent/scripts/generate.js --component",
    "agent:page": "bun .next-agent/scripts/generate.js --page",
    "agent:api": "bun .next-agent/scripts/generate.js --api",
    "typecheck": "tsc --noEmit",
    "clean": "bun run rm -rf .next dist",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject .next && sentry-cli sourcemaps upload .next",
    "build:production": "NODE_ENV=production bunx next build && npm run sentry:sourcemaps",
    "performance:monitor": "bunx next build && bunx next start &"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
